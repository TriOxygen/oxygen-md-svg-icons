{
  "presets":  ["es2015", "stage-1", "react"],
  "plugins":  [
    ["oxygen-css", {
      "vendorPrefixes": true,
      "prefix": "oi",
      "compressClassNames": true,
      "bundleFile": "lib/bundle.css"
      }
    ],
    ["transform-export-extensions"],
    ["transform-class-properties"],
    ["transform-object-rest-spread"]
    ]
}
